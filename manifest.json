{
  "manifest_version": 3,
  "name": "Download Organizer",
  "version": "1.0",
  "description": "Classifies and saves downloads into specific folders.",
  "permissions": [
    "downloads",
    "downloads.shelf",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png"
    }
  }
}
